<%- include("partials/header.ejs") %>
<h1>Home</h1>
<p><%= StartingContent %></p>
<% posts.forEach(function(post) {
  if (post._id && post.username && post.body) { %>
    <h1><%= post.username %></h1>
    <p>
      <%= post.body.substring(0, 100) + "..." %>
      <a href="/posts/<%= post._id %>">Read More</a>
    </p>
<%   }
}); %>
<%- include("partials/footer.ejs") %>
